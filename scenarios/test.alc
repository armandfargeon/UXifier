app dsl {
    theme myTheme {
        colors{
            color black code "#000000"
            color green code "#00FF00"
            color white code "#FFFFFF"
            color graylight code "#ededed"
        } 
    }
    header AppBar title "test" logo "www.image-url.com" level 3 myTheme.black
    hide myhide{
        classic statscovid
    }
    menu MyMenu {
        page myPageOne "pageOne" {
            widgetWrapper widgetWrapperOne width: 100 px {
                classic statscovid
                columnchart statCrypto {
                    filters on the left
                    column width measures "45%"
                    downloadeable "true"
                }
                popup popu1 polarchart statParticipation { 
                    filters on the left
                } => myhide.statscovid
            }
        }
        page myPageOne "pageTwo" {
            widgetWrapper widgetWrapperOne width: 100 px {
                classic statlicenciement
                linechart statCasContact {
                    filters on the left
                }
                polarchart statParticipation { 
                    filters on the left
                }
            }
        }
    }

    plugins{
        DarkMode X left
    }
}